= field_set_tag do
  - entry.used_attributes(%w[kind]).each do |attr|
    = f.labeled(attr) do
      - entry.class::AVAILABLE_KINDS.each do |kind|
        = f.label(:"kind_#{kind.downcase}", class: 'radio inline') do
          = f.radio_button(:kind, kind) 
          = kind

  - entry.used_attributes(%w[unsexed]).each do |attr|
    = f.labeled(attr, "&nbsp;".html_safe) do
      = f.label(:unsexed_true, class: 'radio inline') do
        = f.radio_button(:unsexed, true) 
        = 'Gemischt'

      = f.label(:unsexed_false, class: 'radio inline') do
        = f.radio_button(:unsexed, false) 
        = 'Geschlechtergetrennt'

  - entry.used_attributes(%w[parish bank_account jubla_insurance jubla_full_coverage]).each do |attr|
    = f.labeled_input_field attr

= field_set_tag do
  - entry.used_attributes(%w[coach_id advisor_id]).each do |attr|
    = f.labeled_input_field attr
